/* components/t_navbar/index.wxss */
.custom-header {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}

.custom-header-block {
  display: flex;
}

.custom-header-back {
  position: absolute;
  left: 32rpx;
}

.custom-header-name {
  z-index: 1;
  width: 300rpx;
  height: 50rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  text-align: center;
  line-height: 48rpx;
  color: #000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}